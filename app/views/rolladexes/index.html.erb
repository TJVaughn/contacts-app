<p id="notice"><%= notice %></p>

<h1>Rolladexes</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Telephone</th>
      <th>Twitter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rolladexes.each do |rolladex| %>
      <tr>
        <td><%= rolladex.first_name %></td>
        <td><%= rolladex.last_name %></td>
        <td><%= rolladex.email %></td>
        <td><%= rolladex.telephone %></td>
        <td><%= rolladex.twitter %></td>
        <td><%= link_to 'Show', rolladex %></td>
        <td><%= link_to 'Edit', edit_rolladex_path(rolladex) %></td>
        <td><%= link_to 'Destroy', rolladex, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rolladex', new_rolladex_path %>
